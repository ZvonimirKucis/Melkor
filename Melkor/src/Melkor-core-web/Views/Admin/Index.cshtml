@model Melkor_core_dbhandler.NotificationContext

<div class="center-block">
    @{
        Html.RenderPartial("AddNewsPartial", Model);
    }
</div>

<div class="col-md-12">
    <div class="col-md-6">
        <div class="Tests">
            <div class="partialContents" data-url="@Url.Action("TestResultsPassed", "Admin")">
                <div class="progress progress-striped active">
                    <div class="progress-bar" style="width: 100%"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="Tests">
            <div class="partialContents" data-url="@Url.Action("TestResultsFailed", "Admin")">
                <div class="progress progress-striped active">
                    <div class="progress-bar" style="width: 100%"></div>
                </div>
            </div>
        </div>
    </div>
</div>